		store = store / 16;
		if(store < 16){
			switch(store)
			{
			case 10:
				returnString += "A";
			case 11:
				returnString += "B";
			case 12:
				returnString += "C";
			case 13:
				returnString += "D";
			case 14:
				returnString += "E";
			case 15:
				returnString += "F";
				default:
					returnString += store;
			}
			
		}
	}while(store > 16);
}
